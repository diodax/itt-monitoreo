R IN CONNECTION WITH THE SOFTWARE OR THE USE OR OTHER DEALINGS IN\nTHE SOFTWARE.","maintainers":[{"name":"balderdashy","email":"mike@balderdash.co"},{"name":"vanetix","email":"matmcfarland@gmail.com"},{"name":"particlebanana","email":"particlebanana@gmail.com"}],"time":{"modified":"2014-09-25T19:45:21.297Z","created":"2014-01-11T20:43:44.421Z","0.9.0":"2014-01-11T20:43:44.776Z","0.10.0":"2014-01-20T09:33:36.232Z","0.9.1":"2014-02-02T21:43:33.739Z","0.10.0-rc1":"2014-02-05T18:18:47.500Z","0.10.1":"2014-09-25T19:01:28.236Z"},"author":{"name":"vanetix","email":"matmcfarland@gmail.com"},"repository":{"type":"git","url":"git@github.com:vanetix/waterline-errors"},"readmeFilename":"Readme.md","homepage":"https://github.com/vanetix/waterline-errors","keywords":["waterline","sails","errors"],"bugs":{"url":"https://github.com/vanetix/waterline-errors/issues"},"license":"MIT","_attachments":{},"_etag":"\"5ITZJ0F34JMSSVJDL3A89650E\""}